{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "rm -rf .next && pnpm type-check && next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "lint": "prettier '(src/**/*.{tsx,ts,js}|*.{tsx,ts,js})' --write && eslint --ext js,jsx,ts,tsx --fix && next lint",
    "test": "jest",
    "test:w": "jest --updateSnapshot && jest --watch",
    "postinstall": "husky install",
    "produce": "git checkout main && git pull origin main && git merge dev && git push && git checkout dev"
  },
  "dependencies": {
    "@emotion/cache": "^11.10.5",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.11",
    "@mui/material": "^5.11.11",
    "@mui/styles": "^5.11.11",
    "@reduxjs/toolkit": "^1.9.3",
    "@types/node": "18.14.4",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "axios": "^1.3.4",
    "eslint": "8.35.0",
    "eslint-config-next": "13.2.3",
    "next": "13.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^8.0.5",
    "socket.io-client": "^4.6.1",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.4.0",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "autoprefixer": "^10.4.13",
    "bufferutil": "^4.0.7",
    "cache": "link:@types/@emotion/cache",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.1.2",
    "next-router-mock": "^0.9.2",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "sass": "^1.58.3",
    "tailwindcss": "^3.2.7",
    "typescript-eslint": "0.0.1-alpha.0",
    "utf-8-validate": "^6.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "(src/**/*.{tsx,ts,js}|*.{tsx,ts,js})": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{!(min).js,json,!(min).css,!(min).scss,md}": [
      "prettier --write"
    ]
  }
}
